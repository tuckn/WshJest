{
  "name": "@tuckn/WshJest",
  "description": "The testing module for WSH (Windows Script Host).",
  "version": "3.0.0",
  "author": "Tuckn <tuckn333+github@gmail.com>",
  "private": false,
  "license": "MIT",
  "homepage": "https://docs.tuckn.net/WshJest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuckn/WshJest.git"
  },
  "bugs": {
    "url": "https://github.com/tuckn/WshJest/issues"
  },
  "keywords": [
    "Windows",
    "CLI",
    "WSH",
    "WSF",
    "JScript",
    "VBScript",
    "test",
    "module"
  ],
  "files": [],
  "main": "",
  "bin": {},
  "scripts": {
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-p fix:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml,yaml}\"",
    "lint:type": "tsc --noEmit",
    "test:name:toBe": "cscript //nologo ./Package.wsf //job:test:Jest -t toBe$",
    "test:name:toBeUndefined": "cscript //nologo ./Package.wsf //job:test:Jest -t toBeUndefined$",
    "test:name:toEqual": "cscript //nologo ./Package.wsf //job:test:Jest -t toEqual$",
    "test:name:toContain": "cscript //nologo ./Package.wsf //job:test:Jest -t toContain$",
    "test:name:toContainEqual": "cscript //nologo ./Package.wsf //job:test:Jest -t toContainEqual$",
    "test:Jest": "cscript //nologo ./Package.wsf //job:test:Jest",
    "build": "rimraf ./dist & wsh-packager bundle ./Package.wsf",
    "test:dist:app": "cscript //nologo ./Package.wsf //job:test:dist:app",
    "test": "run-s test:dist:*",
    "predocs": "rimraf ./docs",
    "docs:common": "jsdoc --configure ./jsdoc.json",
    "cp-docs-css": "cpx ./scripts/jsdoc.css ./docs/styles/",
    "cp-docs-img": "cpx ./assets/img ./docs/",
    "postdocs": "run-p cp-docs-*",
    "docs": "run-s docs:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo @FIXME \"npx lint-staged\" fixes all .js!"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "*.{json,css,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@tuckn/wsh-packager": "^1.0.0",
    "@types/activex-scripting": "^1.0.7",
    "@types/windows-script-host": "^5.8.3",
    "cpx": "^1.5.0",
    "docdash": "^1.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "^24.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
}
